@import "../../wuBaseWxss/4.wxss";

.game-container {
    width: 100%;
    height: 100%;
    text-align: center;
}

#question-box {
    position: relative;
    margin: 0 auto;
    width: 700rpx;
    height: 345rpx;
    background: transparent top center no-repeat;
    background-size: 640rpx;
    padding-top: 165rpx;
}

.question-level {
    position: absolute;
    left: 50%;
    top: 30rpx;
    margin-left: -100rpx;
    text-align: center;
    color: #f8e4ce;
    font-size: 36rpx;
    display: inline-block;
    width: 200rpx;
    height: 50rpx;
    line-height: 50rpx;
}

.question-title {
    height: 60rpx;
    font-size: 42rpx;
    color: #cf7200;
    font-weight: bold;
}

.question-answer {
    height: 60rpx;
    margin-top: 35rpx;
    font-size: 38rpx;
    color: #cf7200;
    animation-fill-mode: forwards;
}

.question-btns {
    margin-top: 50rpx;
}

.question-btns image {
    width: 180rpx;
    height: 74rpx;
    padding: 0px;
    margin: 0rpx 3rpx;
}

.question-btns button {
    width: 180rpx;
    height: 74rpx;
    margin: 0rpx 3rpx;
    display: inline-block;
    background: transparent center no-repeat;
    background-size: 100% auto;
}

#option-box {
    width: 700rpx;
    margin: 0 auto;
}

.option-btn {
    display: inline-block;
    width: 86rpx;
    height: 86rpx;
    line-height: 88rpx;
    background: transparent center no-repeat;
    background-size: 82rpx;
    color: #cf7200;
    font-size: 36rpx;
    margin: 0 3rpx 10rpx;
}

.level-text,.level-gap {
    position: absolute;
    left: 460rpx;
    top: 156rpx;
    font-size: 28rpx;
    color: #cd741a;
    width: 220rpx;
    text-align: left;
}

.level-gap {
    left: 544rpx;
    top: 150rpx;
    font-size: 36rpx;
    width: 100rpx;
    text-align: center;
}

.mask-box {
    display: none;
    opacity: 0;
    position: absolute;
    left: 0px;
    top: 0px;
    width: 100%;
    height: 100%;
    background: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAB4AAAAeCAYAAAA7MK6iAAAACXBIWXMAAAsTAAALEwEAmpwYAAAAIGNIUk0AAHolAACAgwAA+f8AAIDpAAB1MAAA6mAAADqYAAAXb5JfxUYAAAAySURBVHja7M0xEQAgDACxB8v1f4cExi6JgZxqWnBbIhaLxWKxWCwWi8XivwcAAP//AwAU6ACvNonlKQAAAABJRU5ErkJggg==);
}

.mask-content {
    position: absolute;
    left: 75rpx;
    top: 250rpx;
    width: 600rpx;
    height: 500rpx;
    background: transparent top center no-repeat;
    background-size: 600rpx auto;
    text-align: center;
}

.mask-title {
    color: #fdf9e2;
    font-size: 38rpx;
    display: block;
    margin-top: 10rpx;
}

.mask-text {
    display: block;
    color: #cf7200;
    font-size: 36rpx;
    margin-top: 75rpx;
    line-height: 54rpx;
}

.mask-num {
    display: block;
    color: #cf7200;
    font-size: 42rpx;
    margin-top: 38rpx;
    line-height: 54rpx;
    text-indent: 80rpx;
    font-weight: bold;
}

.mask-btn {
    width: 180rpx;
    height: 90rpx;
    margin-top: 16rpx;
}

#question-box {
    background-image: url(data:image/png;base64,);
}

.option-btn {
    background-image: url(data:image/png;base64,);
}

.option-checked {
    background-image: url(data:image/png;base64,);
    background-size: 72rpx;
    color: #bf4718;
    font-weight: bold;
}

.mask-content {
    background-image: url(data:image/png;base64,);
}

.question-btns button {
    background-image: url(data:image/png;base64,);
}

#coin-box {
    position: relative;
    display: inline-block;
    margin: 40rpx auto 45rpx;
    left: -20%;
}

#coin-box image {
    width: 200rpx;
    height: 100rpx;
}

#coin-box text {
    position: absolute;
    left: 0;
    top: 0;
    color: #e0705a;
    font-size: 40rpx;
    width: 200rpx;
    height: 100rpx;
    line-height: 105rpx;
    text-indent: 50rpx;
}

#userinfo-box {
    position: fixed;
    width: 100%;
    height: 200rpx;
    background: transparent top center no-repeat;
    background-size: 130rpx auto;
    top: 500rpx;
    left: 80rpx;
}

.userphoto {
    text-align: center;
    padding-top: 18rpx;
}

.userphoto image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100rpx;
}

.level-name {
    color: #f7e9dc;
    font-size: 28rpx;
    line-height: 100rpx;
}

#userinfo-box {
    background-image: url(data:image/png;base64,);
}