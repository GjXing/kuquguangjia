@import "../../wuBaseWxss/4.wxss";

.game-container {
    width: 100%;
    height: 100%;
    text-align: center;
}

.logo {
    margin-bottom: 0rpx;
}

.logo image {
    width: 400rpx;
    margin: 0px auto;
}

.btn-modelevel image,.btn-modechallenge image {
    width: 400rpx;
    height: 140rpx;
}

.btn-modechallenge button {
    width: 400rpx;
    height: 106rpx;
    margin: 14rpx auto;
    display: inline-block;
    background: transparent center no-repeat;
    background-size: 100% auto;
}

#userinfo-box {
    width: 100%;
    height: 200rpx;
    background: transparent top center no-repeat;
    background-size: 130rpx auto;
    top: 120rpx;
    left: 200rpx;
}

.userphoto {
    text-align: center;
    padding-top: 18rpx;
}

.userphoto image {
    width: 100rpx;
    height: 100rpx;
    border-radius: 100rpx;
}

.level-name {
    color: #f7e9dc;
    font-size: 28rpx;
    line-height: 100rpx;
}

#userinfo-box {
    background-image: url(data:image/png;base64,);
}