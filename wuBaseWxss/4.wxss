.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

page {
    background: #e8e2b3;
    width: 100%;
    height: 100%;
    overflow-x: hidden;
    overflow-y: auto;
    font-family: 'Microsoft Yahei',Arial,Helvetica,sans-serif;
}

.aShow {
    display: block!important;
}

.btn-hover {
    transform: scale(0.9,0.9);
}

.game-container {
    background: url(data:image/png;base64,) center no-repeat;
    background-size: 100% auto;
}

.ifav {
    position: fixed;
    top: 0.5rem;
    right: 0.5rem;
    padding: 0.2rem 0.6rem;
    font-size: 0.6rem;
    color: #fff;
    background: #fc0101;
    border-radius: 0.2rem;
    box-shadow: 0 0 0.3rem #fc0101;
    z-index: 10100;
}

.ifd {
    position: absolute;
    top: -0.8rem;
    right: 3.8rem;
    color: #fc0101;
    font-size: 1rem;
}

.share_yuan {
    z-index: 5000;
    width: 90rpx;
    height: 90rpx;
    position: fixed;
    top: 450rpx;
    right: 10rpx;
    font-size: 30rpx;
    background: -webkit-gradient(linear,0% 100%,100% 100%,from(rgba(255,0,0,1)),to(rgba(255,0,200,1)));
    color: #ffffff;
    display: flex;
    justify-content: center;
    align-items: center;
    padding-left: 0;
    padding-right: 0;
    border: 1rpx solid #ffffff;
    border-radius: 50% 50%;
    border-right: none;
    box-shadow: 1px 1px 1px #888888;
    font-weight: bold;
}