.toast_content_box {
    display: flex;
    width: 100%;
    height: 100%;
    justify-content: center;
    align-items: center;
    position: fixed;
    z-index: 999;
}

.toast11 {
    height: 50rpx;
    width: 60rpx;
}

.toast_content {
    width: 62%;
    padding: 15rpx;
    background: rgba(0,0,0,0.8);
    border-radius: 50rpx;
    display: flex;
}

.toast_content_text {
    height: 100%;
    width: 100%;
    color: #fff;
    font-size: 28rpx;
    margin-top: 7rpx;
    margin-left: 20rpx;
}